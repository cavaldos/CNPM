{
  "name": "server",
  "version": "2.1.0",
  "description": "Typescript Project Template for Back End Development",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "ts:start": "ts-node src/server.ts",
    "dev": "tsnd --respawn --transpile-only --exit-child src/server.ts",
    "debug": "tsnd --respawn --transpile-only --exit-child --debug src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "node",
    "nodejs",
    "template",
    "prisma",
    "backend"
  ],
  "author": {
    "name": "Nivaldo Farias",
    "url": "https://github.com/NivaldoFarias"
  },
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2",
    "yarn": ">=1.22.19"
  },
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.17.1",
    "ansi-colors": "^4.1.3",
    "axios": "^1.8.2",
    "chalk": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "firebase-admin": "^13.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mssql": "^11.0.1",
    "nodemailer": "^6.10.0",
    "openai": "^4.86.2",
    "portfinder": "^1.0.32"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/mssql": "^9.1.5",
    "@types/node": "^18.0.3",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "prettier": "^3.5.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^5.8.2"
  }
}
